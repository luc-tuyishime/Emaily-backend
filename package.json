{
  "name": "emaily-server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "10.16.3",
    "npm": "6.11.3"
  },
  "scripts": {
    "start": "babel-node --presets @babel/preset-env ./src/bin/www",
    "server": "babel-node ./src/bin/www",
    "dev": "NODE_ENV=development nodemon --exec npm-run-all server",
    "client": "npm run start --prefix client-emaily",
    "dev-client": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.3.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-proxy-middleware": "^0.20.0",
    "mongoose": "^5.7.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  }
}
